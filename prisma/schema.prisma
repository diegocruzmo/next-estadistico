generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Report {
  id            String   @id @default(uuid())
  userId        String

  month         String   @db.Text
  year          String   @db.Text
  territorial   String   @db.Text

  numcvm        Int
  numcvf        Int
  numcvtm       Int
  numcvtf       Int
  numcvnb       Int
  numcvlg       Int
  numcvin       Int  

  numcpm        Int
  numcpf        Int
  numcptm       Int
  numcptf       Int
  numcpnb       Int
  numcplg       Int
  numcpin       Int 

  numqrm        Int
  numqrf        Int
  numqrtm       Int
  numqrtf       Int
  numqrnb       Int
  numqrlg       Int
  numqrin       Int   

  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
